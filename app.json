{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/yaper-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#025ACE"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.yaper.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/yaper-logo.png",
        "backgroundColor": "#025ACE"
      },
      "package": "co.yaper.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/yaper-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e0ae4d99-4403-4b0c-8e16-a4134b45a84d"
      }
    },
    "owner": "harshosale"
  }
}